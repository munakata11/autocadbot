{
  "name": "autocadassistant",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "kill:port": "powershell -Command \"Get-Process -Id (Get-NetTCPConnection -LocalPort 3000 -ErrorAction SilentlyContinue).OwningProcess | Stop-Process -Force\" || exit 0",
    "kill:win": "taskkill /F /IM electron.exe /T || exit 0 && taskkill /F /IM node.exe /T || exit 0",
    "next-dev": "next dev -p 3000",
    "electron-dev": "electron .",
    "dev": "npm run kill:win && npm run kill:port && concurrently \"npm run next-dev\" \"wait-on tcp:3000 && npm run electron-dev\"",
    "build": "next build && electron-builder",
    "start": "next start",
    "lint": "next lint"
  },
  "main": "electron/main.js",
  "build": {
    "appId": "com.autocadassistant",
    "files": [
      "electron/**/*",
      ".next/**/*",
      "node_modules/**/*"
    ]
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "groq-sdk": "^0.9.1",
    "lucide-react": "^0.469.0",
    "next": "14.2.16",
    "openai": "^4.77.0",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^9.1.2",
    "electron": "^33.3.1",
    "electron-builder": "^25.1.8",
    "eslint": "^8",
    "eslint-config-next": "14.2.16",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "wait-on": "^8.0.1"
  }
}
